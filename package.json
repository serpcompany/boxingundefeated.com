{
  "name": "boxingundefeated",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:remote": "nuxt dev --remote",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "install:clean": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "typecheck": "nuxt typecheck",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio --port 4983",
    "db:studio:prev": "DRIZZLE_ENV=preview drizzle-kit studio --port 4984",
    "db:studio:prod": "DRIZZLE_ENV=production drizzle-kit studio --port 4985"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint": "1.4.1",
    "@nuxt/image": "1.10.0",
    "@nuxthub/core": "^0.9.0",
    "@nuxtjs/seo": "3.1.0",
    "@nuxtjs/sitemap": "^7.4.3",
    "@stylistic/eslint-plugin": "^4.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^22.15.29",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.1.4",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "drizzle-zod": "^0.8.2",
    "eslint": "^9.28.0",
    "eslint-plugin-format": "^1.0.1",
    "eslint-plugin-jsdoc": "^50.6.17",
    "eslint-plugin-vuejs-accessibility": "^2.4.1",
    "nuxt": "^4.0.2",
    "tsx": "^4.20.3",
    "typescript": "^5.3.0",
    "vue-tsc": "^2.2.10",
    "wrangler": "^4.27.0"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.9.2",
    "@nuxt/scripts": "^0.11.10",
    "@nuxt/ui": "^2.22.0",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/nuxt": "^10.5.0",
    "pinia": "^2.1.7",
    "uuid": "^11.1.0",
    "zod": "^3.25.76"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ]
  }
}